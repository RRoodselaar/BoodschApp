<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.6"/>
    <httpRuntime targetFramework="4.6"/>
  </system.web>
  <system.webServer>
   <staticContent>
     <remove fileExtension=".appcache" />
     <mimeMap fileExtension=".appcache" 
               mimeType="text/cache-manifest" />
   </staticContent>
 </system.webServer>
</configuration>